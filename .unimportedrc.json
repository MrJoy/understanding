{
  "entry": [
    "src/app/page.tsx",
    "src/app/layout.tsx",
    "src/app/api/**/route.{js,ts}*"
  ],
  "ignoreUnresolved": [],
  "ignoreUnused": [
    "@tailwindcss/forms"
  ],
  "ignoreUnimported": [],
  "ignorePatterns": [
    ".next/**",
    ".yarn/**",
    "**/__tests__/**",
